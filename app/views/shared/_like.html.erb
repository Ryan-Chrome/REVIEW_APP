<%= form_with(model: current_user.review_relationships.build, remote: true) do |f| %>
    <div><%= hidden_field_tag :review_id, @review.id %></div>
    <div class = "good-btn-content">
        <%= f.submit "good！", class: "like-btn" %>
        <span><i class="fas fa-thumbs-up"></i> <%= @review.review_relationships.count %></span>
    </div>
<% end %>